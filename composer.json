{
  "name": "patryk-sawicki/orlenpaczka-laravel",
  "description": "Orlen Paczka API client for laravel.",
  "license": "MIT",
  "authors": [
    {
      "name": "Patryk Sawicki",
      "email": "patryk@patryksawicki.pl"
    }
  ],
  "autoload": {
    "psr-4": {
      "PatrykSawicki\\OrlenPaczkaApi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PatrykSawicki\\OrlenPaczkaTests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.2",
    "ext-json": "*",
    "ext-simplexml": "*",
    "laravel/framework": "^10.00"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "laravel/laravel": "^10.00 || ^11.00"
  },
  "extra": {
    "laravel": {
      "providers": [
        "PatrykSawicki\\OrlenPaczkaApi\\app\\Providers\\OrlenPaczkaServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  }
}
